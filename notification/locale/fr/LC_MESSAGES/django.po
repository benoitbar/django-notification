#: templates/notification/email_body.txt:1
#, python-format
msgid ""
"You have received the following notice from %(current_site)s:\n"
"\n"
"%(message)s\n"
"\n"
"To see other notices or change how you receive notifications, please go to %(default_http_protocol)s://%(current_site)s%(notices_url)s\n"
msgstr ""
"Vous avez reçu la notification suivante du site %(current_site)s\n"
"\n"
"%(message)s\n"
"\n"
"Pour voir les autres notifications ou changer la façon dont vous recevez ses notifications, veuillez vous rendre sur %(default_http_protocol)s://%(current_site)s%(notices_url)s\n"

#: templates/notification/email_subject.txt:1
#, python-format
msgid "[%(current_site)s] %(message)s"
msgstr "[%(current_site)s] %(message)s"

